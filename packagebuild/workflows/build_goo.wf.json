{
  "Name": "goo",
  "Vars": {
    "gcs_path": {
      "Required": true
    },
    "repo_owner": {
      "Required": true
    },
    "repo_name": {
      "Required": true
    },
    "git_ref": {
      "Required": true
    },
    "extra_repo": {
      "Required": false
    },
    "extra_repo_owner": {
      "Required": false
    },
    "extra_git_ref": {
      "Required": false
    },
    "version": {
      "Required": true
    },
    "build_dir": {
      "Required": true
    },
    "sbom_util_gcs_root": {
      "Required": false
    },
    "lkg_gcs_path": {
      "Required": false,
      "Value": {}
    }
  },
  "Steps": {
    "build-package": {
      "SubWorkflow": {
        "Path": "./build_package.wf.json",
        "Vars": {
          "type": "goo",
          "sourceImage": "projects/debian-cloud/global/images/family/debian-12",
          "gcs_path": "${gcs_path}",
          "repo_owner": "${repo_owner}",
          "repo_name": "${repo_name}",
          "git_ref": "${git_ref}",
          "extra_repo": "${extra_repo}",
	  "extra_repo_owner": "${extra_repo_owner}",
          "extra_git_ref": "${extra_git_ref}",
          "build_dir": "${build_dir}",
          "version": "${version}",
          "sbom_util_gcs_root": "${sbom_util_gcs_root}",
	  "lkg_gcs_path": "${lkg_gcs_path}"
        }
      }
    }
  }
}
